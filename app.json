{
  "name": "binimg-app",
  "description": "تطبيق Node.js على Heroku لربوت binimg",
  "scripts": {
    "start": "node index.js"
  },
  "version": "1.0.0",
  "engines": {
    "node": "^18.0.6"
  },
  "dependencies": {
    "@types/node": "^18.0.6",
    "axios": "^1.6.7",
    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "env": {
    "PAGE_ACCESS_TOKEN": {
      "description": "Access token for Facebook page",
      "required": false
    },
    "VERIFY_TOKEN": {
      "description": "Verification token for Facebook webhook",
      "required": false
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/mxschmitt/heroku-playwright-buildpack.git"
    },
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku-community/apt"
    }
  ],
  "repository": "https://github.com/hh/hh",
  "keywords": ["Node.js", "Heroku"],
  "license": "رخصة-البرنامج"
}
